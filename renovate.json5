{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended",
    ":dependencyDashboard"
  ],
  timezone: 'Asia/Tokyo',
  prConcurrentLimit: 20,
  prHourlyLimit: 10,
  "reviewers": [
    "Soli0222"
  ],
  semanticCommits: 'enabled',
  prCreation: 'immediate',
  rebaseWhen: 'auto',
  dependencyDashboard: true,
  vulnerabilityAlerts: {
    enabled: true,
    labels: [
      'security',
    ],
  },
}
